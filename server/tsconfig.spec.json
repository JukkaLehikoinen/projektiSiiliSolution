{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["es2020"],
    "types": ["jest", "node", "sequelize","express"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "allowJs": true,
    "noLib": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
}
